{
   "name": "esp32-motion-control",
   "version": "0.0.0",
   "private": true,
   "scripts": {
      "dev": "vite",
      "build": "vue-tsc --noEmit && vite build",
      "deploy": "vue-tsc --noEmit && vite build && rm dist/assets/*.js dist/assets/*.css dist/*.html && node deploy.js",
      "preview": "vite preview",
      "lint": "eslint . --fix --ignore-path .gitignore",
      "generate:api": "openapi-generator-cli generate -i ../openapi/openapi-description.yaml -g typescript-axios -o ./src/generated/api"
   },
   "dependencies": {
      "@babel/types": "^7.20.7",
      "@mdi/font": "7.0.96",
      "axios": "^1.3.3",
      "core-js": "^3.8.3",
      "io-ts": "^2.2.20",
      "lodash": "^4.17.21",
      "mime-types": "^2.1.35",
      "pinia": "^2.0.23",
      "roboto-fontface": "*",
      "ts-node": "^10.9.1",
      "vue": "^3.2.13",
      "vue-router": "^4.0.0",
      "vuedraggable": "^4.1.0",
      "vuetify": "^3.0.0",
      "webfontloader": "^1.0.0"
   },
   "devDependencies": {
      "@mdi/js": "^7.1.96",
      "@openapitools/openapi-generator-cli": "^2.5.2",
      "@types/axios": "^0.14.0",
      "@types/lodash": "^4.14.194",
      "@types/mime-types": "^2.1.1",
      "@types/node": "^18.11.9",
      "@types/vuedraggable": "^2.24.0",
      "@types/webfontloader": "^1.6.35",
      "@vitejs/plugin-vue": "^3.0.3",
      "@vue/eslint-config-typescript": "^11.0.0",
      "eslint": "^8.22.0",
      "eslint-plugin-vue": "^9.3.0",
      "nanoid": "^3.3.6",
      "rollup-plugin-gzip": "^3.1.0",
      "sass": "^1.55.0",
      "typescript": "^4.9.5",
      "vite": "^3.1.9",
      "vite-plugin-vuetify": "^1.0.0-alpha.12",
      "vue-tsc": "^1.0.9"
   }
}
